
<div class="container-white">
  <h1 class="road-trip-title"><%= @road_trip.title %></h1>
  <%= image_tag @road_trip.photo, class: "trip-image" %>

  <div class="p-description">
    <p><%= @road_trip.description %>
    </p>
  </div>

  <div id='map' style='width: 1008px; height: 600px;'></div>


  <div class="d-flex-date">
    <%# <div>From <%= @road_trip.joins(:points).where('points.start_date')</div> %>
    <%# <div>To <%= @road_trip.joins(:points).where('points.end_date')</div> %>
  </div>


  <div class="d-flex">
    <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://images.pexels.com/photos/12510146/pexels-photo-12510146.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://images.pexels.com/photos/4595612/pexels-photo-4595612.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://images.pexels.com/photos/11525541/pexels-photo-11525541.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

  </div>

</div>
